get_discounted_price <- function(quantity, unit_price){
discount = 0
total_price = unit_price*quantity
if( quantity > 25 &  total_price > 5000){
discount = 0.1
} else if( quantity > 25 |  total_price > 5000){
discount = 0.05
}
netprice = total_price - total_price*discount
print(paste("Net price is ", netprice))
}
